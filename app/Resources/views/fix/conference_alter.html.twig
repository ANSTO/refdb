<div class="modal-header">
    <h5 class="modal-title">Fix conference details</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <button type="submit" class="btn btn-primary">Submit</button>
    {{ form_end(form) }}
    <br />
    <a class="btn btn-default btn-modal" href="{{ path('fix_conference', {"id": reference.id }) }}"><i class="fas fa-arrow-left"></i> Back </a>
    <a href="javascript:void(0)" class="btn btn-default btn-block" data-dismiss="modal">Cancel</a>
</div>